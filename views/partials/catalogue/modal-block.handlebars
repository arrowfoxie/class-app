<div class="regModal d-none">
  <div class="regModal-content">
    <div class="container justify-content-center">
      <form action="/register" method="post">
        <div class="register mx-auto col-sm-10 col-md-8 col-lg-6 col-xl-5">
          <h2 class="text-center mt-3">Register</h2>
          <ul class="addedClasses mx-auto reg">
          </ul>
          <h6 class="mb-0 ml-2">Tuition: $<span class="subtotal">0.00</span></h6>
          <h6 class="mb-0 ml-2 my-2">Fees: $<span id="fees"></span></h6>
          <h6 class="mb-0 ml-2">Total: $<span id="total"></span></h6>
          <button class="btn bg-blue text-white w-25 mt-4 mr-3" type="submit">Register</button>
          <button class="btn bg-red text-white w-25 mt-4" type="submit">Cancel</button>
        </div>
      </form>
    </div>
  </div>
</div>